{
    "name": "bet-tracker/app",
    "description": "Sistema de rastreamento de apostas com OCR",
    "type": "project",
    "require": {
        "php": "^8.0",
        "ext-pdo": "*",
        "ext-gd": "*",
        "thiagoalessio/tesseract_ocr": "^2.13",
        "phpoffice/phpspreadsheet": "^1.29"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
