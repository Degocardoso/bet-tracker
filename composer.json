{
    "name": "bet-tracker/app",
    "description": "Sistema de rastreamento de apostas com OCR",
    "type": "project",
    "require": {
        "php": "^8.0",
        "thiagoalessio/tesseract_ocr": "^2.13",
        "phpoffice/phpspreadsheet": "^1.29",
        "ext-pdo": "*",
        "ext-gd": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
